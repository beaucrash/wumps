#include <iostream>
#include <ctime>
#include <cstdlib>

using namespace std;

void gerarTabuleiro(char tabuleiro[][8]){
   for(int i=0; i<8; i++){
      for(int j=0; j<8; j++){
         tabuleiro[i][j] = '0';
      }
   }
}

void imprimirTabuleiro(char tabuleiro[][8]){
   for(int i=0; i<8; i++){
      for(int j=0; j<8; j++){
         cout << tabuleiro[i][j] << " ";
      }
      
